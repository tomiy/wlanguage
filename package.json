{
  "name": "windev-wlanguage",
  "displayName": "Windev WLanguage",
  "description": "WLanguage support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "wlanguage",
      "aliases": ["WLanguage", "wlanguage"],
      "extensions": ["(.wd",".wdp",".wdg",".wdc",".wdw",".wpf",".wdr)"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wlanguage",
      "scopeName": "source.wlanguage",
      "path": "./syntaxes/wlanguage.tmLanguage.json"
    }]
  }
}
